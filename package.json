{
  "name": "twitch-bot",
  "version": "0.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "nodemon ./lib/bot.js",
    "build": "tsc --watch --preserveWatchOutput",
    "lint": "./node_modules/.bin/eslint **/*.{js,ts,tsx}",
    "test": "ava",
    "coverage": "nyc ava"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/patomation/twitch-bot.git"
  },
  "keywords": [
    "twitch",
    "chat",
    "bot"
  ],
  "author": "Patrick Kelly",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/patomation/twitch-bot/issues"
  },
  "homepage": "https://github.com/patomation/twitch-bot#readme",
  "dependencies": {
    "dotenv": "^8.2.0",
    "tmi.js": "^1.5.0"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@types/tmi.js": "^1.4.0",
    "@typescript-eslint/eslint-plugin": "^3.2.0",
    "@typescript-eslint/parser": "^3.2.0",
    "ava": "^3.9.0",
    "eslint": "^7.2.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^4.2.5",
    "nodemon": "^2.0.4",
    "nyc": "^15.1.0",
    "ts-node": "^8.10.2",
    "tsconfigs": "^5.0.0",
    "typescript": "^3.9.5"
  }
}
